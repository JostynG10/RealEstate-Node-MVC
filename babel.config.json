{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@src": "./src",
          "@routes": "./src/routes",
          "@views": "./src/views",
          "@controllers": "./src/controllers"
        }
      }
    ]
  ]
}
